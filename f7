function largestOfFour(arr) {
  let largestArr = [];
for (let i = 0; i < arr.length; i ++) {
  let tempLarge = arr[i][0];
  for (let j = 0; j < arr[i].length; j++) {
    let num = arr[i][j];
    if (num >= tempLarge) {
      tempLarge = num
    }
  }
  largestArr.push(tempLarge)
}
  return largestArr;
}
